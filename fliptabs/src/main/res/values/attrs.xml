<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlipTab">
        <attr name="flipAnimationDuration" format="integer" />
        <attr name="wobbleReturnAnimationDuration" format="integer" />
        <attr name="wobbleAngle" format="float" />
        <attr name="overallColor" format="color" />
        <attr name="highlightColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="leftTabText" format="string" />
        <attr name="rightTabText" format="string" />
        <attr name="startingTab" format="enum" >
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>